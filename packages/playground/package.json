  "dependencies": {
    "@monaco-editor/react": "^4.4.4",
    "modern-css-reset": "^1.4.0",
    "prettier": "^2.6.2",
    "prismjs": "^1.28.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@types/prettier": "^2.6.0",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
