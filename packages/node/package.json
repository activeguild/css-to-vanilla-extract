{
  "name": "c2ve",
  "version": "0.0.17-alpha",
  "description": "Convert css (scss/sass) to vanilla-extract for node.",
  "main": "index.js",
  "bin": {
    "c2ve": "dist/index.js"
  },
  "scripts": {
    "build": "cargo-cp-artifact -nc ./dist/index.node -- cargo build --target x86_64-apple-darwin --message-format=json-render-diagnostics",
    "build-release": "npm run build -- --release && cp -r ./index.js ./dist",
    "test-dir": "node ./dist/index.js ./samples",
    "test-file": "node ./dist/index.js ./samples/test.css",
    "test-css": "node -r esbuild-register ./samples/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activeguild/css-to-vanilla-extract.git"
  },
  "keywords": [
    "css",
    "scss",
    "sass",
    "convert",
    "vanilla-extract",
    "typescript",
    "react"
  ],
  "author": "j1ngzoue",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/activeguild/css-to-vanilla-extract/issues"
  },
  "homepage": "https://github.com/activeguild/css-to-vanilla-extract#readme",
  "dependencies": {
    "@vanilla-extract/css": "^1.7.0",
    "commander": "^9.2.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.27",
    "@vanilla-extract/esbuild-plugin": "^2.0.3",
    "cargo-cp-artifact": "^0.1.6",
    "esbuild": "^0.14.38",
    "esbuild-register": "^3.3.2"
  }
}
